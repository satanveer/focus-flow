# Production Environment Variables
# Copy this file to .env.production and fill in your production values

# Appwrite Configuration (Production)
VITE_APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
VITE_APPWRITE_PROJECT_ID=your_production_project_id
VITE_APPWRITE_DATABASE_ID=your_production_database_id

# Collection IDs (Production)
VITE_APPWRITE_TASKS_COLLECTION_ID=your_production_tasks_collection
VITE_APPWRITE_SESSIONS_COLLECTION_ID=your_production_sessions_collection
VITE_APPWRITE_NOTES_COLLECTION_ID=your_production_notes_collection
VITE_APPWRITE_FOLDERS_COLLECTION_ID=your_production_folders_collection
VITE_APPWRITE_SETTINGS_COLLECTION_ID=your_production_settings_collection
VITE_APPWRITE_CALENDAR_EVENTS_COLLECTION_ID=your_production_calendar_collection
VITE_APPWRITE_TIME_BLOCKS_COLLECTION_ID=your_production_timeblocks_collection
VITE_APPWRITE_PRODUCTIVITY_GOALS_COLLECTION_ID=your_production_goals_collection
VITE_APPWRITE_GOOGLE_TOKENS_COLLECTION_ID=your_production_tokens_collection

# Google Calendar OAuth (Production)
VITE_GOOGLE_CALENDAR_CLIENT_ID=your_production_google_client_id.apps.googleusercontent.com
VITE_GOOGLE_CALENDAR_REDIRECT_URI=https://yourdomain.com/auth/google-calendar

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_LOG_LEVEL=error

# Performance
VITE_ENABLE_SERVICE_WORKER=true
